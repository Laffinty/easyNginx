# 用户自定义的Nginx配置
events {
    worker_connections 2048;
    use select;
}

http {
    include mime.types;
    
    # 用户自定义http配置
    client_max_body_size 50m;
    keepalive_timeout 65s;
    
    # 用户自己的server
    server {
        listen 8080;
        server_name user.local;
        root html/user;
        index index.html;
    }
}
